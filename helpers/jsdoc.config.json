{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": [
      "./.tiny_source_code/src/"
    ]
  },
  "plugins": [
    "./node_modules/jsdoc/plugins/markdown",
    "./helpers/jsdoc-fix"
  ],
  "templates": {
    "default": {
      "outputSourceFiles": true
    },
    "applicationName": "",
    "footer": "Maintained by the Tiny team",
    "copyright": "Tiny",
    "linenums": true
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "opts": {
    "template": "./helpers/jsdoc-template",
    "destination": "./.tiny_source_code/api/",
    "recurse": true
  }
}
